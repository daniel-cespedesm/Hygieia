pipeline {
  agent { label "Hygieia-node" }
  stages {
    stage('Build') {
      steps {
        sh 'mvn clean install'
      }
    }
    stage('Dockerize') {
      steps {
        sh 'docker-compose build'
      }
    }
    stage('Start up') {
      steps {
        sh 'export DOCKER_HOST=127.0.0.1:2375'
        sh 'docker-compose up -d'
      }
    }
  }
}
