## This file triggers the pipeline to build, dockerize and start up Hygieia

pipeline {
  agent { label "Hygieia-node" }
  stages {
    stage('Build') {
      steps {
        sh 'mvn clean install'
      }
    }
    stage('Dockerize') {
      steps {
        sh 'docker-compose build'
      }
    }
    stage('Start up') {
      steps {
        sh 'docker-compose up -d'
      }
    }
  }
}
